{
  "expo": {
    "name": "Marina",
    "slug": "Marina",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "Dozvolite aplikaciji pristup galeriji kako biste mogli dodavati sadržaj."
        }
      ],
      [
        "expo-camera",
        {
          "cameraPermission": "Dozvolite aplikaciji pristup kameri kako biste mogli dodavati sadržaj."
        }
      ],
      [
        "expo-media-library",
        {
          "mediaLibraryPermission": "Dozvolite aplikaciji pristup galeriji kako biste mogli dodavati sadržaj."
        }
      ]
    ]
  }
}
